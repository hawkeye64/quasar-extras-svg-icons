{
  "name": "quasar-extras-svg-icons-mono-repo",
  "version": "1.0.0",
  "description": "Quasar SVG Icons mono-repo",
  "workspaces": [
    "icons",
    "docs"
  ],
  "author": "Jeff Galbraith <galbraith64@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn install --ignore-engines && cd ./docs && yarn build",
